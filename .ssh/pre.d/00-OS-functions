# -*- mode: sh -*-

set_term_for_sp3 () {
    case "$TERM" in
        rxvt-256color|rxvt-unicode-256color|screen-256color*)
            TERM=screen-256color
            ;;
    esac
}

set_term_for_godaddy () {
    case "$TERM" in
        rxvt-unicode-256color)
            TERM=rxvt-256color
            ;;
        screen-256color-bce-it)
            TERM=screen-256color-bce
            ;;
    esac
}
